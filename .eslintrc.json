{
    "extends": "standard",
    "parser": "typescript-eslint-parser",
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module"
    },
    "rules": {
        "comma-dangle": "off",
        "no-undef": "off",
        "no-unused-vars": "off",
        "space-before-function-paren": "off"
    }
}
